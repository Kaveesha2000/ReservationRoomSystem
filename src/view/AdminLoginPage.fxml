<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="adminContext" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminLoginPageController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" opacity="0.85" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/loginpic1.jpg" />
         </image>
      </ImageView>
      <JFXTextField fx:id="adminName" layoutX="29.0" layoutY="185.0" onAction="#moveToPassword" prefHeight="25.0" prefWidth="545.0" promptText="Name :- " style="-fx-background-color: white;">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </JFXTextField>
      <JFXPasswordField fx:id="adminPassword" layoutX="29.0" layoutY="242.0" prefHeight="25.0" prefWidth="545.0" promptText="Password :- " style="-fx-background-color: white;">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </JFXPasswordField>
      <Label layoutX="241.0" layoutY="38.0" text="Admin Log In" textFill="WHITE">
         <font>
            <Font name="Bodoni MT Bold Italic" size="20.0" />
         </font>
      </Label>
      <Button layoutX="17.0" layoutY="13.0" mnemonicParsing="false" onAction="#backToHome" prefHeight="25.0" prefWidth="99.0" style="-fx-background-color: black;" text="Back To Home" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="479.0" layoutY="350.0" mnemonicParsing="false" onAction="#logInToAdminForm" prefHeight="25.0" prefWidth="99.0" style="-fx-background-color: blue;" text="Log In" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Label fx:id="adminLabel" layoutX="30.0" layoutY="297.0" prefHeight="17.0" prefWidth="545.0" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <ImageView fitHeight="69.0" fitWidth="164.0" layoutX="220.0" layoutY="92.0" opacity="0.85" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/logo.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
